{
  "name": "collab",
  "version": "1.0.0",
  "description": "trello clone",
  "main": "index.js",
  "engines": {
    "node": "11.10.1",
    "npm": "6.7.0"
  },
  "scripts": {
    "sequelize-skeleton": "./node_modules/sequelize-cli/lib/sequelize init:models; ./node_modules/sequelize-cli/lib/sequelize init:config",
    "webpack": "webpack --watch ---open --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "server": "babel-node ./src/index",
    "server:prod": "node ./app/index",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf app",
    "build": "babel ./src --out-dir app",
    "prod": "npm-run-all clean build server:prod",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "start": "concurrently \"npm run prod\" \"npm run frontend\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "apollo-boost": "^0.4.4",
    "apollo-server-express": "^2.9.3",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "graphql": "^14.5.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "merge-graphql-schemas": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "react": "^16.9.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.0",
    "sequelize": "^5.18.2",
    "sequelize-cli": "^5.5.1",
    "styled-components": "^4.4.0",
    "uuid": "^3.3.3",
    "validator": "^11.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "css-loader": "^3.2.0",
    "nodemon": "^1.19.2"
  }
}
